<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.revoreva.maze" version="1.0.0.0">

	<name>Maze</name>
	<description>Wonder around Maze</description>
